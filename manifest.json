{
  "manifest_version": 2,
  "name": "Goofy Ah Extension",
  "version": "1.5",
  "permissions": ["tabs", "webNavigation"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
